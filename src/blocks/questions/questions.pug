mixin questions(icon)
	+section({
		mods: "questions,s-small",
		mainClass: "questions"
	})
		.questions__wrapper
			.questions__upgrade
				.questions__icon-wrapper
					+icon(icon, 'questions__icon')
				a.questions__link.js-modal-link(href="#popup-questions") Что можно улучшить на этой странице?
			block
