extends pug/base

append vars
	- html.classList.push('page-inner')
	- var title = 'Специальный предложения'
block header
	+header()
block content
	+breadcrumbs({mods: 'catalog'})
		+breadcrumbs-item-icon('home')
		+breadcrumbs-item-nav({
			label: 'Специальный предложения',
		})
	+actions()
	+you-seen()
		+questions('question')
			+question-social()
block footer
	+footer()
	+overlay()
